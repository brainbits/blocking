{
    "name": "brainbits/blocking",
    "description": "Object blocking",
    "keywords": ["block", "blocking", "async"],
    "homepage": "http://brainbits.net",
    "license": "MIT",
    "authors": [
        {
            "name": "Stephan Wentz",
            "email": "swentz@brainbits.net"
        }
    ],
    "require": {
        "php": "^8.2",
        "psr/clock": "^1.0"
    },
    "require-dev": {
        "brainbits/phpcs-standard": "^7.0.1",
        "brainbits/phpstan-rules": "^3.1.2",
        "matthiasnoback/symfony-config-test": "^4.3",
        "matthiasnoback/symfony-dependency-injection-test": "^4.3.1",
        "mikey179/vfsstream": "^1.6.11",
        "phpstan/phpstan": "^1.10.67",
        "phpunit/phpunit": "^10.5.19",
        "predis/predis": "^2.2.2",
        "symfony/clock": "^6.4.5",
        "symfony/config": "^6.4.6",
        "symfony/dependency-injection": "^6.4.6",
        "symfony/http-foundation": "^6.4.4",
        "symfony/http-kernel": "^6.4.6",
        "symfony/routing": "^6.4.6",
        "symfony/security-core": "^6.4.3",
        "phpstan/phpstan-phpunit": "^1.3.16",
        "phpstan/phpstan-symfony": "^1.3.12"
    },
    "suggest": {
        "predis/predis": "If you want to use the PredisStorage",
        "symfony/http-foundation": "If you want to use the SymfonySessionOwnerFactory",
        "symfony/security-core": "If you want to use the SymfonyTokenOwnerFactory"
    },
    "conflict": {
        "phpspec/prophecy": "1.12.*"
    },
    "autoload": {
        "psr-4": { "Brainbits\\Blocking\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
